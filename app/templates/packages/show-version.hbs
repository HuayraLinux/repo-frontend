{{#repo-breadcrumbs}}
  {{#link-to "index"}}distributions{{/link-to}} /
  {{#link-to "distributions.show" model.distributionName}}{{model.distributionName}}{{/link-to}} /
  {{model.packageName}}
{{/repo-breadcrumbs}}

<h2 class="ui header">Paquete
  {{#link-to "packages.showPackage" model.packageName (query-params distribution=model.distributionName)}}{{model.packageName}}{{/link-to}}
   ({{model.version}}) en {{#link-to "distributions.show" model.distributionName}}{{model.distributionName}}{{/link-to}}</h2>

<p>{{model.Description}}</p>

<ul>
  <li>{{#link-to "packages.showPackage" model.packageName (query-params distribution=model.distributionName)}}Ver qu√© versiones hay en otras distribuciones{{/link-to}}</li>

  {{#if model.striderURL}}
    <li><a href={{model.striderURL}} target=_blank>URL al proyecto en strider</a></li>
  {{/if}}

  {{#if model.striderJobURL}}
    <li><a href={{model.striderJobURL}} target=_blank>URL al build de strider</a></li>
  {{/if}}

  {{#if model.repoURL}}
    <li><a href={{model.repoURL}} target=_blank>Link al repositorio</a></li>
  {{/if}}

  {{#if model.repoPackageURL}}
    <li><a href={{model.repoPackageURL}} target=_blank>Link a repositorio (del paquete)</a></li>
  {{/if}}

  {{#if model.lastCommitURL}}
    <li><a href={{model.lastCommitURL}} target=_blank>Link al commit relativo al build</a></li>
  {{/if}}

  {{#if model.DownloadURL_i386}}
    <li><a href={{model.DownloadURL_i386}} target=_blank>Descargar .deb para i386</a></li>
  {{/if}}

  {{#if model.DownloadURL_amd64}}
    <li><a href={{model.DownloadURL_amd64}} target=_blank>Descargar .deb para amd64</a></li>
  {{/if}}


</ul>

<p><b>Maintainer:</b> {{model.Maintainer}}</p>
<p><b>Depends:</b> {{repo-inline-package-list distro=model.distributionName packages=model.Depends}}</p>
<p><b>Conflicts:</b> {{repo-inline-package-list distro=model.distributionName packages=model.Conflicts}}</p>
<p><b>Priority:</b> {{model.Priority}}</p>
<p><b>Section:</b> {{model.Section}}</p>
